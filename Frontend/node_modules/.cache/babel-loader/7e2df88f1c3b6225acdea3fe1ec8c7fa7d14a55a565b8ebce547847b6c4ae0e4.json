\n            await axios.post(API_BASE_URL + '/notes', this.$data)\n            .then( response => {\n                this.title = '',\n                this.content = '',\n                this.isLoading = false,\n                console.debug('response', response.data.data)\n                this.$emit('completed', response.data.data)\n            })\n            .catch(error => {\n                this.errors = error.response.data.errors\n                this.isLoading = false\n            })\n        }\n    }\n}\n</script>\n\n<style>\n\n</style>"],"mappings":"AAgBA,OAAOA,KAAI,MAAO,OAAO;AACzB,SAASC,YAAW,QAAS,UAAU;AAEvC,eAAe;EACXC,IAAIA,CAAA,EAAG;IACH,OAAO;MACHC,KAAK,EAAE,EAAE;MACTC,OAAO,EAAE,EAAE;MACXC,MAAM,EAAE,EAAE;MACVC,SAAS,EAAE;IACf;EACJ,CAAC;EACDC,OAAO,EAAE;IACLC,QAAQA,CAAA,EAAG;MACP,IAAI,CAACF,SAAQ,GAAI,IAAG;MACpB,IAAI,CAACG,WAAW,CAAC;IACrB,CAAC;IACD,MAAMA,WAAWA,CAAA,EAAG;MAChB,MAAMT,KAAK,CAACU,IAAI,CAACT,YAAW,GAAI,QAAQ,EAAE,IAAI,CAACU,KAAK,EACnDC,IAAI,CAAEC,QAAO,IAAK;QACf,IAAI,CAACV,KAAI,GAAI,EAAE,EACf,IAAI,CAACC,OAAM,GAAI,EAAE,EACjB,IAAI,CAACE,SAAQ,GAAI,KAAK,EACtBQ,OAAO,CAACC,KAAK,CAAC,UAAU,EAAEF,QAAQ,CAACX,IAAI,CAACA,IAAI;QAC5C,IAAI,CAACc,KAAK,CAAC,WAAW,EAAEH,QAAQ,CAACX,IAAI,CAACA,IAAI;MAC9C,CAAC,EACAe,KAAK,CAACC,KAAI,IAAK;QACZ,IAAI,CAACb,MAAK,GAAIa,KAAK,CAACL,QAAQ,CAACX,IAAI,CAACG,MAAK;QACvC,IAAI,CAACC,SAAQ,GAAI,KAAI;MACzB,CAAC;IACL;EACJ;AACJ"},"metadata":{},"sourceType":"module","externalDependencies":[]}