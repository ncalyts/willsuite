{"ast":null,"code":"import axios from 'axios';\nimport { API_BASE_URL } from '@/config';\nexport default {\n  data() {\n    return {\n      title: '',\n      content: '',\n      id: '',\n      buttonText: 'Add Note',\n      errors: '',\n      isLoading: false\n    };\n  },\n  emits: [\"completed\"],\n  props: ['noteId', 'noteTitle', 'noteContent', 'editButtonText'],\n  watch: {\n    noteId(newId) {\n      this.id = newId;\n    },\n    noteTitle(newTitle) {\n      this.title = newTitle;\n    },\n    noteContent(newContent) {\n      this.content = newContent;\n    },\n    editButtonText(newContent) {\n      this.buttonText = newContent;\n    }\n  },\n  methods: {\n    onSubmit() {\n      this.isLoading = true;\n      if (this.id == '') {\n        this.postNewNote();\n      } else {\n        this.updateNote();\n      }\n    },\n    async postNewNote() {\n      await axios.post(API_BASE_URL + '/notes', this.$data).then(response => {\n        this.title = '', this.content = '', this.id = '', this.isLoading = false, this.$emit('completed', response.data.data);\n      }).catch(error => {\n        this.errors = error.response.data.errors;\n        this.isLoading = false;\n      });\n    },\n    async updateNote() {\n      await axios.put(API_BASE_URL + '/notes/' + this.id, this.$data).then(response => {\n        this.title = '', this.content = '', this.buttonText = 'Add Note', this.id = '', this.isLoading = false, this.$emit('updated', response.data.data);\n      });\n    }\n  }\n};","map":{"version":3,"names":["axios","API_BASE_URL","data","title","content","id","buttonText","errors","isLoading","emits","props","watch","noteId","newId","noteTitle","newTitle","noteContent","newContent","editButtonText","methods","onSubmit","postNewNote","updateNote","post","$data","then","response","$emit","catch","error","put"],"sources":["/Users/neilclayton/Documents/Dev/laravel/willsuite/frontend-vue/src/components/NoteForm.vue"],"sourcesContent":["<template>\n    <div class=\"flex flex-col justify-center items-center\">\n        <form class=\"bg-white shadow-md rounded px-8 pt-6 pb-8 mb-4\" @submit.prevent=\"onSubmit\">\n            <span v-text=\"errors\"></span>\n            <div class=\"mb-4\">\n                <label class=\"block text-gray-700 text-sm font-bold mb-2\" for=\"title\">Title</label>\n                <input class=\"shadow appearance-none border rounded w-full py-2 px-3 text-gray-700 leading-tight focus:outline-none focus:shadow-outline\" id=\"title\" type=\"text\" name=\"title\" v-model=\"title\" required @keydown=\"errors = ''\">\n            </div>\n            <div class=\"mb-6\">\n                <label for=\"content\">Content</label>\n                <input id=\"content\" type=\"textarea\" name=\"content\" v-model=\"content\">\n            </div>\n            <input v-model=\"id\" type=\"hidden\">\n            <button v-bind:class=\"{'isLoading' : isLoading}\">{{ buttonText }}</button>\n        </form>\n    </div>\n</template>\n\n<script>\nimport axios from 'axios';\nimport { API_BASE_URL } from '@/config';\n\nexport default {\n    data() {\n        return {\n            title: '',\n            content: '',\n            id: '',\n            buttonText: 'Add Note',\n            errors: '',\n            isLoading: false\n        }\n    },\n    emits: [\"completed\"],\n    props: ['noteId', 'noteTitle', 'noteContent', 'editButtonText'],\n    watch: {\n        noteId(newId) {\n            this.id = newId;\n        },\n        noteTitle(newTitle) {\n            this.title = newTitle;\n        },\n        noteContent(newContent) {\n            this.content = newContent;\n        },\n        editButtonText(newContent) {\n            this.buttonText = newContent;\n        }\n    },\n    methods: {\n        onSubmit() {\n            this.isLoading = true\n            if (this.id == '') {\n                this.postNewNote()\n            } else {\n                this.updateNote()\n            }\n        },\n        async postNewNote() {\n            await axios.post(API_BASE_URL + '/notes', this.$data)\n            .then( response => {\n                this.title = '',\n                this.content = '',\n                this.id = '', \n                this.isLoading = false,\n                this.$emit('completed', response.data.data)\n            })\n            .catch(error => {\n                this.errors = error.response.data.errors\n                this.isLoading = false\n            })\n        },\n        async updateNote() {\n            await axios.put(API_BASE_URL + '/notes/' + this.id, this.$data)\n            .then (response => {\n                this.title = '',\n                this.content = '',\n                this.buttonText = 'Add Note',\n                this.id = '',\n                this.isLoading = false, \n                this.$emit('updated', response.data.data)\n            })\n        }\n    }\n}\n</script>\n\n<style>\n\n</style>"],"mappings":"AAmBA,OAAOA,KAAI,MAAO,OAAO;AACzB,SAASC,YAAW,QAAS,UAAU;AAEvC,eAAe;EACXC,IAAIA,CAAA,EAAG;IACH,OAAO;MACHC,KAAK,EAAE,EAAE;MACTC,OAAO,EAAE,EAAE;MACXC,EAAE,EAAE,EAAE;MACNC,UAAU,EAAE,UAAU;MACtBC,MAAM,EAAE,EAAE;MACVC,SAAS,EAAE;IACf;EACJ,CAAC;EACDC,KAAK,EAAE,CAAC,WAAW,CAAC;EACpBC,KAAK,EAAE,CAAC,QAAQ,EAAE,WAAW,EAAE,aAAa,EAAE,gBAAgB,CAAC;EAC/DC,KAAK,EAAE;IACHC,MAAMA,CAACC,KAAK,EAAE;MACV,IAAI,CAACR,EAAC,GAAIQ,KAAK;IACnB,CAAC;IACDC,SAASA,CAACC,QAAQ,EAAE;MAChB,IAAI,CAACZ,KAAI,GAAIY,QAAQ;IACzB,CAAC;IACDC,WAAWA,CAACC,UAAU,EAAE;MACpB,IAAI,CAACb,OAAM,GAAIa,UAAU;IAC7B,CAAC;IACDC,cAAcA,CAACD,UAAU,EAAE;MACvB,IAAI,CAACX,UAAS,GAAIW,UAAU;IAChC;EACJ,CAAC;EACDE,OAAO,EAAE;IACLC,QAAQA,CAAA,EAAG;MACP,IAAI,CAACZ,SAAQ,GAAI,IAAG;MACpB,IAAI,IAAI,CAACH,EAAC,IAAK,EAAE,EAAE;QACf,IAAI,CAACgB,WAAW,CAAC;MACrB,OAAO;QACH,IAAI,CAACC,UAAU,CAAC;MACpB;IACJ,CAAC;IACD,MAAMD,WAAWA,CAAA,EAAG;MAChB,MAAMrB,KAAK,CAACuB,IAAI,CAACtB,YAAW,GAAI,QAAQ,EAAE,IAAI,CAACuB,KAAK,EACnDC,IAAI,CAAEC,QAAO,IAAK;QACf,IAAI,CAACvB,KAAI,GAAI,EAAE,EACf,IAAI,CAACC,OAAM,GAAI,EAAE,EACjB,IAAI,CAACC,EAAC,GA